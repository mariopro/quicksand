{
    "name": "mariopro/quicksand",
    "description": "Easily schedule regular cleanup of old soft-deleted Eloquent data.",
    "homepage": "https://github.com/mariopro/quicksand",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Pro"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/console": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/log": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/legacy-factories": "^1.4.0",
        "mockery/mockery": "^1.6.0",
        "orchestra/testbench": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0",
        "phpunit/phpunit": "^8.5 || ^9.0 || ^10.5"
    },
    "autoload": {
        "psr-4": {
            "MarioPro\\Quicksand\\": "src"
        },
        "classmap": [
            "tests"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "MarioPro\\Quicksand\\QuicksandServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
